                    </div><!-- / col -->

                </div><!-- / Row -->
            </div> *@
            <!-- / project-status-area -->
            <!-- project of the month -->
            <section class="project-ofthe-month sec-pt">
                <div class="row">
                    <div class="col-12">
                        <div class="card ation-card">
                            <div class="card-header">
                                <h3>Alumini Details</h3>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-bordered" style="width: 100%;">
                                        <thead>
                                            <tr>
                                                <th>Action</th>
                                                <th>Name</th>
                                                <th>Organization</th>
                                                <th>Profession</th>
                                                <th>Passout Year</th>
                                                <th>Marital Status</th>
                                                <th>Email</th>
                                                <th>DOB</th>
                                                <th>Mobile No.</th>
                                                <th>Address</th>
                                               
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in Model.aluminiList)
                                            {
                                            <tr>
                                                <td>
                                                   <a class='btn btn-sm btn-info btn-xs waves-effect waves-light' href="@Url.Action("approve_alumini","Vsk_admin",new {regno=item.registration_number })">Approve</a>
                                                </td>
                                                <td>
                                                    @item.name
                                                </td>
                                                <td>
                                                    @item.organisation
                                                </td>
                                                <td>
                                                   @item.profession
                                                </td>
                                                <td>
                                                    @item.passout_year
                                                </td>
                                                <td>
                                                    @item.marital_status
                                                </td>
                                                <td>
                                                    <span>@item.eamil</span>
                                                </td>
                                                <td>
                                                    @Convert.ToDateTime(item.dob).ToString("dd/MM/yyyy")
                                                </td>
                                                <td>
                                                    @item.mobile
                                                </td>
                                                <td>
                                                    @item.address
                                                </td>
                                               
                                            </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- /project of the month -->
            <!-- /section -->

        </div>
    </div>

    <!-- End of Main content Wrapper
