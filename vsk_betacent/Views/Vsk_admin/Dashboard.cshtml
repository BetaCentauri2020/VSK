    <!-- Start Main content Wrapper
    ============================================== -->

    <div class="content-wrapper">
        <div class="container-fluid">

            <!-- Breadcrumb -->
            <div class="page-breadcrumb">
                <div class="row align-items-center">
                    <div class="col-md-6">
                        <h4>Dashboard</h4>
                    </div>
                    <div class="col-md-6">
                        <ul>
                            <li><a href="index.html">Dashboard</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- /Breadcrumb -->
            <!-- Statistics -->
            <section class="ation-statistics">
                <div class="row">

                    <div class="col-xl-4 col-md-6 col-12 mb-xl-0 mb-4">
                        <div class="single-statistics">
                            <div class="card ation-card">
                                <div class="card-header bg-primary">
                                    <h3 class="text-white">Inquiry</h3>
                                </div>
                                <div class="card-body">
                                    <div class="content">
                                        <div class="left">
                                            <h3 class="text-primary">@Model.totalInquiry</h3>
                                        </div>
                                        <div class="right">
                                            <span class="circle"></span>
                                            <img src="~/assets/images/icon-feedback.png">
                                        </div>
                                    </div><!-- /content -->
                                </div>
                            </div>
                        </div>
                        <!--/single-statistics-->
                    </div>
                    <!--/col-->

                    <div class="col-xl-4 col-md-6 col-12 mb-md-0 mb-4">
                        <div class="single-statistics">
                            <div class="card ation-card">
                                <div class="card-header bg-success">
                                    <h3 class="text-white">Total Staff</h3>
                                </div>
                                <div class="card-body">
                                    <div class="content">
                                        <div class="left">
                                            <h3 class="text-success">@Model.totalStaff</h3>
                                        </div>
                                        <div class="right">
                                            <span class="circle"></span>
                                            <img src="~/assets/images/icon-staff.png">
                                        </div>
                                    </div><!-- /content -->
                                </div>
                            </div>
                        </div>
                        <!--/single-statistics-->
                    </div>
                    <!--/col-->

                    <div class="col-xl-4 col-md-6 col-12">
                        <div class="single-statistics">
                            <div class="card ation-card">
                                <div class="card-header bg-warning">
                                    <h3 class="text-white">Alumni</h3>
                                </div>
                                <div class="card-body">
                                    <div class="content">
                                        <div class="left">
                                            <h3 class="text-warning">@Model.totalAlumini</h3>
                                        </div>
                                        <div class="right">
                                            <span class="circle"></span>
                                            <img src="~/assets/images/icon-alumni.png">
                                        </div>
                                    </div><!-- /content -->
                                </div>
                            </div>
                        </div>
                        <!--/single-statistics-->
                    </div>
                    <!--/col-->

                </div>
                <!--/Row-->
            </section>
            <!-- /Statistics -->
            <!--  project-status-area -->
